@model List<MyELearningProject.DAL.Entities.Service>
<div class="col-lg-6 wow fadeInUp" data-wow-delay="0.3s">
    <h6 class="section-title bg-white text-start text-primary pe-3">Hizmetlerimiz</h6>
    <h1 class="mb-4">sizinle olmaktan mutluyuz</h1>
    <p class="mb-4">bizimle beraber calıştığınız süre zarfında ve sonrasında sizlere sağladığımız imkanlar</p>
    
    <div class="row gy-2 gx-4 mb-4">
        @foreach (var item in Model)
        {
            <div class="col-sm-6">
                <p class="mb-0"><i class="fa fa-arrow-right text-primary me-2"></i>@item.servicetittle</p>
            </div>
        }


    </div>
   
</div>
